{
    "version": 2,
    "builds": [
        {
            "src": "dist/app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/app.js",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
        }
    ],
    "buildCommand": "rm -rf dist && npm install && npm run build",
    "cleanUrls": true
}
