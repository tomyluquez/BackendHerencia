{
    "version": 2,
    "builds": [
        {
            "src": "dist/app.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["dist/**", ".env"]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/app.js"
        }
    ],
    "buildCommand": "npm install && npm run build"
}
